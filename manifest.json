{
  "name": "Focus",
  "description": "Easily focus on reading articles by eliminating distractions and focusing on what matters.",
  "version": "0.1",
  "manifest_version": 2,
  "icons": { "512": "focus512.png", "128": "focus128.png", "48": "focus48.png", "16": "focus16.png"},
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["./content/css/jquery-ui.min.css","./content/css/focus.css"],
      "js": ["./content/js/jquery.min.js","./content/js/jquery-ui.min.js", "./content/js/focus.js"],
      "run_at": "document_start"
    }
  ]
}
